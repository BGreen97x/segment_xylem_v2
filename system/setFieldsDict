FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0 //orig 0
);

regions
(

    boxToCell
    {
        box (0 0 -1) (1 0.006 1); 
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }

    sphereToCell
    {
        centre (0 0.003 0); //(0 0.003 0)
        radius 0.001; //0.0005 
        fieldValues
        (
            volScalarFieldValue alpha.water 0
        );
    }
);


// ************************************************************************* //
